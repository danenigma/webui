	   <script>

		function outputUpdate_neck_one(vol) {
			document.querySelector('#neck_servo_one').value = vol;
			  var values = vol;
		          jQuery.ajax({
					  url:"/neck_servo_one/",
			    		  cache: false,
			    data: {
				'values': values
			    },
			    type: 'POST'
			});
		}
		function outputUpdate_neck_two(vol) {
			document.querySelector('#neck_servo_two').value = vol;
			  var values = vol;
		          jQuery.ajax({
					  url:"/neck_servo_two/",
			    		  cache: false,
			    data: {
				'values': values
			    },
			    type: 'POST'
			});
		}	
		function outputUpdate_eyes_pan(vol) {
			document.querySelector('#eyes_pan').value = vol;
			  var values = vol;
		          jQuery.ajax({
					  url:"/eyes_pan/",
			    		  cache: false,
			    data: {
				'values': values
			    },
			    type: 'POST'
			});
		}	
		function outputUpdate_eyes_tilt(vol) {
			document.querySelector('#eyes_tilt').value = vol;
			  var values = vol;
		          jQuery.ajax({
					  url:"/eyes_tilt/",
			    		  cache: false,
			    data: {
				'values': values
			    },
			    type: 'POST'
			});
		}
	    </script>
